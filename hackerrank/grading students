#include<stdio.h>
int main()
{
    int n,i,j,k;
    scanf("%d",&n);
    int arr[n];//creates an integer array of size 'n'
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);//taking user input 
    }
    for(j=0;j<n;j++)
    {
        for(k=40;k<=100;k+=5)
        {
            if(k>arr[j])
            {
                if((k-arr[j])<3)
                {
                    arr[j] = k;
                }
                break;
            }

        }
    }
    for(int l=0;l<n;l++)
    {
        printf("%d\n",arr[l]);//prints final output 
    }
    
}
